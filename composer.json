{
    "name": "enjoyscms/hybridauth",
    "description": "Social login (oauth) module for EnjoysCMS",
    "type": "enjoyscms-module",
    "require": {
        "php": "^8.1",
        "enjoyscms/core": "^6.0@dev",
        "enjoyscms/admin": "^6.0@dev",
        "hybridauth/hybridauth": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "EnjoysCMS\\Module\\Hybridauth\\": "src/"
        }
    },
    "extra": {
        "module-name": "Hybridauth"
    },
    "require-dev": {
        "vimeo/psalm": "^4.0 | ^5.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    }
}
